<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="BakalarskaPracePU" transaction-type="JTA">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <jta-data-source>jdbc/fel</jta-data-source>
    <class>dbEntity.DenVTydnu</class>
    <class>dbEntity.GroupTable</class>
    <class>dbEntity.Mistnost</class>
    <class>dbEntity.Predmety</class>
    <class>dbEntity.RezervaceMistnosti</class>
    <class>dbEntity.Rozvrhy</class>
    <class>dbEntity.Semestr</class>
    <class>dbEntity.Stredisko</class>
    <class>dbEntity.UpdateRozvrhu</class>
    <class>dbEntity.Uzivatel</class>
    <class>dbEntity.TypMistnosti</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
